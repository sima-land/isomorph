    "eslint": "^8.53.0",
    "fs-extra": "^11.1.1",
    "husky": "^8.0.3",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "prettier": "^3.0.3",
    "typescript": "^5.3.2",
    "whatwg-fetch": "^3.6.17"
  },
