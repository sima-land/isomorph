{
  "name": "@sima-land/isomorph",
  "version": "0.0.0",
  "description": "Tiny framework for fast start frontend microservices",
  "author": "www.sima-land.ru team",
  "license": "Apache-2.0",
  "scripts": {
    "prepare": "husky install",
    "type-check": "tsc -p . --noEmit",
    "build:before": "rm -rf dist",
    "build:main": "tsc --project tsconfig.build.json",
    "build:after": "cp package.json dist/package.json",
    "build": "npm run build:before && npm run build:main && npm run build:after",
    "lint": "eslint --cache ./src --ext .js,.jsx,.ts,.tsx",
    "test": "jest",
    "test:coverage": "jest --clearCache && jest --coverage"
  },
  "engines": {
    "node": ">=16.15.1"
  },
  "devDependencies": {
    "@sima-land/linters": "^2.1.5",
    "@swc/jest": "^0.2.22",
    "@testing-library/react": "^12.1.3",
    "@types/ejs": "^3.1.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.7",
    "@types/jsesc": "^3.0.1",
    "@types/lodash": "^4.14.184",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "@types/webpack": "^5.28.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@humanwhocodes/env": "^2.2.2",
    "@opentelemetry/api": "^1.1.0",
    "@opentelemetry/exporter-jaeger": "^1.5.0",
    "@opentelemetry/instrumentation": "^0.31.0",
    "@opentelemetry/instrumentation-express": "^0.30.0",
    "@opentelemetry/instrumentation-http": "^0.31.0",
    "@opentelemetry/resources": "^1.5.0",
    "@opentelemetry/sdk-trace-base": "^1.6.0",
    "@opentelemetry/sdk-trace-node": "^1.5.0",
    "@opentelemetry/semantic-conventions": "^1.5.0",
    "@opentelemetry/tracing": "^0.24.0",
    "@sentry/browser": "^7.16.0",
    "@sentry/node": "^7.16.0",
    "dotenv": "^16.0.0",
    "ejs": "^3.1.8",
    "express": "^4.17.3",
    "jsesc": "^3.0.2",
    "middleware-axios": "^2.1.2",
    "pino": "^8.4.2",
    "pino-pretty": "^9.0.1",
    "prom-client": "^14.0.1",
    "webpack": "^5.74.0"
  },
  "peerDependencies": {
    "@reduxjs/toolkit": "^1.8.5",
    "axios": "^0.24.0",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^8.0.2",
    "redux-saga": "^1.2.1"
  }
}
