{
  "name": "@dev-dep/isomorph",
  "version": "0.0.0",
  "description": "Server side rendering for frontend microservices",
  "main": "index.js",
  "repository": "ssh://git@gitlab.sima-land.ru:7888/dev-dep/dev/isomorph.git",
  "author": "www.sima-land.ru team",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=10.15.3"
  },
  "publishConfig": {
    "registry": "https://gitlab.sima-land.ru/api/v4/projects/676/packages/npm/"
  },
  "scripts": {
    "build:before": "rimraf build",
    "build:main": "tsc --project tsconfig.build.json",
    "build:after": "node ./dist.js",
    "build": "yarn build:before && yarn build:main && yarn build:after",
    "type-check": "tsc -p . --noEmit",
    "dev": "cd ./examples/hello-world/ && yarn && yarn dev",
    "lint": "eslint --cache ./src --ext .js,.jsx,.ts,.tsx",
    "test": "jest",
    "coverage": "jest --clearCache && jest --coverage"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn jest --coverage",
      "pre-commit": "yarn lint-staged"
    }
  },
  "dependencies": {
    "axios": "0.19.2",
    "express-http-proxy": "^1.5.1",
    "ioredis": "^4.10.0",
    "jaeger-client": "^3.15.0",
    "middleware-axios": "^1.0.0",
    "opentracing": "^0.14.3",
    "pino": "^5.13.1",
    "pino-pretty": "^3.2.0",
    "prom-client": "^11.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.2",
    "@babel/plugin-proposal-optional-chaining": "^7.14.2",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@dev-dep/linters": "^1.0.11",
    "@sentry/browser": "^5.15.5",
    "@sentry/node": "5.10.2",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.169",
    "@types/react": "^16.14.0",
    "@types/react-dom": "^17.0.5",
    "@types/react-redux": "^7.1.16",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "babel-jest": "^26.6.3",
    "copyfiles": "^2.4.1",
    "dotenv": "^8.2.0",
    "enzyme": "3.8.x",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "3.3.x",
    "husky": "2.3.x",
    "identity-obj-proxy": "3.0.x",
    "jest": "^26.6.3",
    "lint-staged": "^10.0.8",
    "lodash": "4.17.15",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-redux": "7.1.3",
    "redux": "4.0.5",
    "redux-saga": "1.1.3",
    "reduxsauce": "1.1.2",
    "reselect": "4.0.0",
    "rimraf": "^3.0.2",
    "sentry-testkit": "^3.2.1",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2.4"
  },
  "peerDependencies": {
    "lodash": "4.17.15",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-redux": "7.1.3",
    "redux": "4.0.5",
    "redux-saga": "1.1.3",
    "reduxsauce": "1.1.2",
    "reselect": "4.0.0"
  }
