{
  "name": "@sima-land/isomorph",
  "version": "0.0.0",
  "description": "Tiny framework for fast start frontend microservices",
  "author": "www.sima-land.ru team",
  "license": "Apache-2.0",
  "scripts": {
    "prepare": "husky install",
    "type-check": "tsc -p . --noEmit",
    "build:before": "rm -rf dist",
    "build:main": "tsc --project tsconfig.build.json",
    "build:after": "cp package.json dist/package.json && cd dist && npm pkg delete scripts.prepare",
    "build": "npm run build:before && npm run build:main && npm run build:after",
    "lint": "eslint --cache ./src --ext .js,.jsx,.ts,.tsx",
    "test": "jest",
    "test:coverage": "jest --clearCache && jest --coverage"
  },
  "engines": {
    "node": ">=16.15.1"
  },
  "devDependencies": {
    "@sima-land/linters": "^2.1.5",
    "@swc/jest": "^0.2.26",
    "@testing-library/react": "^12.1.3",
    "@types/ejs": "^3.1.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^28.1.7",
    "@types/jsesc": "^3.0.1",
    "@types/lodash": "^4.14.194",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "@types/webpack": "^5.28.1",
    "husky": "^8.0.3",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@humanwhocodes/env": "^2.2.2",
    "@krutoo/fetch-tools": "^0.0.7",
    "@opentelemetry/api": "^1.4.1",
    "@opentelemetry/exporter-prometheus": "^0.38.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.39.1",
    "@opentelemetry/propagator-jaeger": "^1.12.0",
    "@opentelemetry/resources": "^1.12.0",
    "@opentelemetry/sdk-metrics": "^1.12.0",
    "@opentelemetry/sdk-trace-base": "^1.12.0",
    "@opentelemetry/sdk-trace-node": "^1.12.0",
    "@opentelemetry/semantic-conventions": "^1.12.0",
    "@sentry/browser": "^7.51.2",
    "@sentry/node": "^7.51.2",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "jsesc": "^3.0.2",
    "middleware-axios": "^2.1.5",
    "pino": "^8.14.1",
    "pino-pretty": "^9.1.1",
    "prom-client": "^14.2.0",
    "webpack": "^5.82.0"
  },
  "peerDependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.4.0",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^8.0.5",
    "redux-saga": "^1.2.3"
  }
}
