{
  "name": "@sima-land/isomorph",
  "version": "0.0.0",
  "description": "Tiny framework for fast start frontend microservices",
  "author": "www.sima-land.ru team",
  "license": "Apache-2.0",
  "scripts": {
    "prepare": "husky install",
    "type-check": "tsc -p . --noEmit",
    "build:before": "rm -rf dist",
    "build:main": "tsc --project tsconfig.build.json",
    "build:after": "cp package.json dist/package.json",
    "build": "npm run build:before && npm run build:main && npm run build:after",
    "lint": "eslint --cache ./src --ext .js,.jsx,.ts,.tsx",
    "test": "jest",
    "test:coverage": "jest --clearCache && jest --coverage"
  },
  "devDependencies": {
    "@sima-land/linters": "^2.1.2",
    "@swc/jest": "^0.2.20",
    "@testing-library/react": "^12.1.3",
    "@types/ejs": "^3.1.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.182",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "@types/webpack": "^5.28.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@humanwhocodes/env": "^2.2.0",
    "@opentelemetry/api": "^1.1.0",
    "@opentelemetry/exporter-jaeger": "^1.0.1",
    "@opentelemetry/instrumentation": "^0.27.0",
    "@opentelemetry/instrumentation-express": "^0.28.0",
    "@opentelemetry/instrumentation-http": "^0.27.0",
    "@opentelemetry/resources": "^1.0.1",
    "@opentelemetry/sdk-trace-node": "^1.0.1",
    "@opentelemetry/semantic-conventions": "^1.0.1",
    "@opentelemetry/tracing": "^0.24.0",
    "@sentry/browser": "^6.19.3",
    "@sentry/node": "^6.19.3",
    "dotenv": "^16.0.0",
    "ejs": "^3.1.8",
    "express": "^4.17.3",
    "middleware-axios": "^2.1.2",
    "pino": "^7.9.1",
    "pino-pretty": "^7.5.4",
    "prom-client": "^14.0.1",
    "webpack": "^5.72.0"
  },
  "peerDependencies": {
    "@reduxjs/toolkit": "^1.8.0",
    "axios": "^0.24.0",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "redux-saga": "^1.1.3"
  }
}
